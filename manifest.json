{
    "name": "tobi tabi",
    "description": "atob the sites",
    "version": "0.1",
    "icons" : {
      "64": "icon.png"
    },
    "permissions": [
    "tabs","<all_urls>"
    ],
    "content_scripts": [
        {
          "matches": [
              "http://*/*",
              "https://*/*"
              ],
          "css": [
            "tobitabi.css"
          ],
          "js": [
            "src/underscore-min.js", "src/jquery.js", "src/tripcode.js",
            "src/md5.js",
            "tobitabi.js"
          ],
          "run_at": "document_start"
        },




        {
          "matches": [
              "http://*.reddit.com/*",
              "https://*.reddit.com/*"
              ],
          "run_at": "document_start",
          "js": [
            "sites/reddit.js"
          ]
        },
        {
          "matches": [
              "http://*.euphoria.io/*",
              "https://*.euphoria.io/*"
              ],
          "run_at": "document_start",
          "js": [
            "sites/euphoria.js"
          ]
        },
        {
          "matches": [
              "http://*.facebook.com/*",
              "https://*.facebook.com/*"
              ],
          "run_at": "document_start",
          "js": [
            "sites/facebook.js"
          ]
        }
    ],
    "manifest_version":2
}

